{
  "listeners": {
    "*:8080": {
      "pass": "routes"
    }
  },

  "routes": [
    {
      "action": {
        "pass": "applications/csp_report"
      }
    }
  ],

  "applications": {
    "csp_report": {
      "type": "php",
      "root": "/var/www/html/src/public/",
      "script": "csp-report.php"
    }
  },
  "access_log": {
    "path": "/var/log/unit/access.log"
  }
}
